<p id="notice"><%= notice %></p>

<h1>Add To Invoice Clients</h1>

<table>
  <thead>
    <tr>
      <th>Document id</th>
      <th>Product id</th>
      <th>Quantity</th>
      <th>Previous quantity</th>
      <th>Post quantity</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @add_to_invoice_clients.each do |add_to_invoice_client| %>
      <tr>
        <td><%= add_to_invoice_client.Document_ID %></td>
        <td><%= add_to_invoice_client.Product_Id %></td>
        <td><%= add_to_invoice_client.Quantity %></td>
        <td><%= add_to_invoice_client.Previous_Quantity %></td>
        <td><%= add_to_invoice_client.Post_Quantity %></td>
        <td><%= link_to 'Show', add_to_invoice_client %></td>
        <td><%= link_to 'Edit', edit_add_to_invoice_client_path(add_to_invoice_client) %></td>
        <td><%= link_to 'Destroy', add_to_invoice_client, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Add To Invoice Client', new_add_to_invoice_client_path %>
