<p id="notice"><%= notice %></p>

<h1>Add To Invoice Suppliers</h1>

<table>
  <thead>
    <tr>
      <th>Document id</th>
      <th>Product id</th>
      <th>Quantity</th>
      <th>Previous quantity</th>
      <th>Post quantity</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @add_to_invoice_suppliers.each do |add_to_invoice_supplier| %>
      <tr>
        <td><%= add_to_invoice_supplier.Document_ID %></td>
        <td><%= add_to_invoice_supplier.Product_Id %></td>
        <td><%= add_to_invoice_supplier.Quantity %></td>
        <td><%= add_to_invoice_supplier.Previous_Quantity %></td>
        <td><%= add_to_invoice_supplier.Post_Quantity %></td>
        <td><%= link_to 'Show', add_to_invoice_supplier %></td>
        <td><%= link_to 'Edit', edit_add_to_invoice_supplier_path(add_to_invoice_supplier) %></td>
        <td><%= link_to 'Destroy', add_to_invoice_supplier, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Add To Invoice Supplier', new_add_to_invoice_supplier_path %>
